<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>Wiktionary</os:ShortName>
<os:Description>Wiktionary</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/png;base64,</os:Image>
<SearchForm>https://en.wiktionary.org/wiki/Wiktionary:Main_Page</SearchForm>
<os:Url type="text/html" method="GET" template="https://en.wiktionary.org/wiki/Special:Search">
  <os:Param name="search" value="{searchTerms}"/>
  <os:Param name="go" value="Look+up"/>
</os:Url>
</SearchPlugin>